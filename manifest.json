{
    "manifest_version": 2,
    "name": "Personal customizations",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": ["https://www.google.com/calendar/render*"],
            "js": ["scripts/dist/google-calendar.js"],
            "css": ["styles/google-calendar.css"]
        }, {
            "matches": ["https://trello.com/*"],
            "css": ["styles/trello.css"]
        }, {
            "matches": ["<all_urls>"],
            "js": ["scripts/dist/ux-distance.js"]
        }
    ],
    "web_accessible_resources": [
        "styles/google-calendar-bubble.css"
    ]
}